@page
@model Importør.Pages.Car.UpdateCarModel
@{
}
<h1>Update the Car!</h1>
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.Car.CarId" class="control-lable"></label>
                <input asp-for="@Model.Car.CarId" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.Model" class="control-lable"></label>
                <input asp-for="@Model.Car.Model" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.Price" class="control-lable"></label>
                <input asp-for="@Model.Car.Price" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.Location" class="control-lable"></label>
                <input asp-for="@Model.Car.Location" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.Type" class="control-lable"></label>
                <input asp-for="@Model.Car.Type" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.FuelType" class="control-lable"></label>
                <input asp-for="@Model.Car.FuelType" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="@Model.Car.Year" class="control-lable"></label>
                <input asp-for="@Model.Car.Year" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-page="/Car/GetAllCars">Back to all Cars</a>
</div>